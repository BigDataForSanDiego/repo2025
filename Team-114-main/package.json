{
  "name": "resourcify",
  "version": "1.0.0",
  "description": "AI-powered resource analysis tool",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server.js",
    "dev:client": "vite",
    "build": "npm install && npx vite build",
    "prod": "npm run build && npm start",
    "heroku-postbuild": "npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase": "^12.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.4.21",
    "@vitejs/plugin-react": "^4.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruvbantval/Resourcify.git"
  },
  "keywords": [
    "ai",
    "resource-management",
    "image-recognition",
    "google-vision",
    "react",
    "emergency-preparedness"
  ],
  "author": "dhruvbantval",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dhruvbantval/Resourcify/issues"
  },
  "homepage": "https://github.com/dhruvbantval/Resourcify#readme"
}
